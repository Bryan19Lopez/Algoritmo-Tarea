//5. Dado un valor de compra presentar el total considerando un 12% de iva
class ejercicio5{
    presentareltotal(){
        console.log("5. Dado un valor de compra presentar el total considerando un 12% de iva")
        let precio=300
        let total=0
        let iva=0.12
        iva=precio*iva
        total=precio+iva
        console.log("total de compra es", total)
    }
}
let total= new ejercicio5()
total.presentareltotal()

//6. Presentar 5 veces un nombre
class ejercicio6{
    presentar5veces(){
        console.log("6. Presentar 5 veces un nombre")
        let nombre="Luciano"
        let contador=1
        console.log("nombre:Luciano")
        for(contador=1; contador <=5; contador++){
            console.log(`${contador} ${nombre}`)
        }
    }
}
let nombre= new ejercicio6()
nombre.presentar5veces()

//7. Presentar los numeros multiplos de 3 del 3 al 21
class ejercicio7{
    multiplosde3(){
        console.log("7. Presentar los numeros multiplos de 3 del 3 al 21")
        let num=1
        while (num<=21){
            if (num%3==0){
                console.log(num)
            }
            num= num+1
        }
    }
}
let presentarnumeros= new ejercicio7()
presentarnumeros.multiplosde3()

