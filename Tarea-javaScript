//10. Dado diez numeros en un arreglo presentar los que tengan menos de 5 caracteres
class ejercicio10{
    presentararreglo(){
        console.log("10. Dado diez numeros en un arreglo presentar los que tengan menos de 5 caracteres")
        let arreglo=["343243","654556","12","10","7","9","877998","477343","8","245542"]
        console.log("el arreglo es:",arreglo)
        for(let pos=0;pos<10;pos=pos+1){
            if(arreglo[pos].length<5){
                console.log(`pos->${pos}elemento:${arreglo[pos]}`)
            }
        } 
    }
}
let caracteres= new ejercicio10()
caracteres.presentararreglo()

