//21. Calcular la serie de fibonacci dado un numero
class ejercicio21{
    calcularseriedefibonacci(){
        console.log("21. Calcular la serie de fibonacci dado un numero")
        let a=0
        let b=1
        let c=1
        let cont=3
        let num=8
        while(cont<num){
            a=b;b=c;c=a+b
            console.log(c)
            cont=cont+1
        }
    }
} 
let fibonacci= new ejercicio21()
fibonacci.calcularseriedefibonacci()

//22. Dado un numero invertirlo
class ejercicio22{
    numeroinvert(){
        console.log("22. Dado un numero invertirlo")
        let num=321
        let ninvertido=0,d=0
        console.log(num)
        while(num !=0){
            d= num%10
            ninvertido= ninvertido*10+d
            num= Math.trunc(num/10)
        }
        console.log(ninvertido)
    }
}
let invertido= new ejercicio22()
invertido.numeroinvert()

//23. Presentar los divisores de un numero
class ejercicio23{
    presentarlosdiv(){
        console.log("23. Presentar los divisores de un numero")
        let num=8
        let c=1
        for(c=1;c<=num;c++){
            if(num%c==0){
                console.log("Los divisores son:",c)
            }
        }
    }
}
let numdediv= new ejercicio23()
numdediv.presentarlosdiv()

//24. Presentar si un numero es perfecto o no
class ejercicio24{
    numeroperfecto(){
        console.log("24. Presentar si un numero es perfecto o no")
        let divisores=0
        let num=6,i=0,sumaperf=0
        for(i=1;i<num;i=i+1){
            if(num%i==0){
                console.log(i)
                sumaperf= sumaperf+i
            }
        }
        console.log("")
        if(sumaperf==num){
            console.log(num,"Es perfecto")
        }
        else{
            console.log(num,"No es perfecto")
        }
    }
}
let presentarperf= new ejercicio24()
presentarperf.numeroperfecto()

//25. Verificar si un numero es primo o no
class ejercicio25{
    verificarprimo(){
        console.log("25. Verificar si un numero es primo o no")
        let num=11
        let cont=0,i=1
        for(i=1;i<num;i++)
        {
        if(num%i==0)
        {
            cont= cont+1
        }
        }
        if(cont=2)
        {
            console.log(num,"Si es primo")
        }
        else
        {
            console.log(num,"No es primo")
        }
    }
}
let numprimo= new ejercicio25()
numprimo.verificarprimo()

