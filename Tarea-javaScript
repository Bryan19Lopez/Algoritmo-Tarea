//16. Presentar la tabla de multiplicar de cualquier numero del 1 al 12
class ejercicio16{
    tablademultiplicar(){
        console.log("16. Presentar la tabla de multiplicar de cualquier numero del 1 al 12")
        console.log("La tabla del 2")
        let n=2
        let n1=1,mul=1
        while(n1<=12){
            mul= n*n1
            console.log(n,"*",n1,"=",mul)
            n1= n1+1 
        }
    }
}
let tabla= new ejercicio16()
tabla.tablademultiplicar()

//17. Realizar la multiplicacion de dos numeros por medio de sumas sucesivas
class ejercicio17{
    multiplicaciondedosnum(){
        console.log("17. Realizar la mutiplicacion de dos numeros por medio de sumas sucesivas")
        let n1=5
        let n2=3
        let suma=0
        for(let i=0;i<n2;i=i+1){
            suma= n1+suma
            console.log(suma-n1,"+",n1,"=",suma)
        }
        console.log("la mutiplicacion de",n1,"*",n2,"es =",suma)
    }
}
let sumasucesivas= new ejercicio17()
sumasucesivas.multiplicaciondedosnum()

//18. Realizar la division de dos numeros por medio de restas sucesivas
class ejercicio18{
    divisiondedosnum(){
        console.log("18. Realizar la division de dos numeros por medio de restas sucesivas")
        let n1=10
        let n2=2
        let c=0
        let res=n1
        while(res-n2>=0){
            res= res-n2
            console.log(res+n2,"-",n2,"=",res)
            c= c+1
        }
        console.log("La division etre:",n1,"/",n2,"=",c)
    }
}
let restasucesivas= new ejercicio18()
restasucesivas.divisiondedosnum()

//19. Calcular el factorial de un numero
class ejercicio19{
    factorialdeunnum(){
        console.log("19. Calcular el factorial de un numero")
        let n=4
        let c=1,fac=1
        while(c<=n){
            fac= fac*c
            c= c+1
        }
        console.log("El factorial del numero 4 es:",fac)
    }
}
let calcularnum= new ejercicio19()
calcularnum.factorialdeunnum()

